<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.networking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/s"
        android:label="CoronaTracker"
        android:roundIcon="@drawable/s"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".about_activity"></activity>
        <activity android:name=".global_show_Activity" />
        <activity android:name=".ShowActivity" />
        <activity android:name=".Global_Activity" />
        <activity android:name=".District" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>